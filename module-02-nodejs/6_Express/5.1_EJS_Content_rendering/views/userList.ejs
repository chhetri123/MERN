<%- include("partials/header.ejs") %>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Email</th>
      <th scope="col">Message</th>
    </tr>
  </thead>
  <tbody>
    <% userList.forEach((user,index)=>{ %>
    <tr>
      <th scope="row"><%= index+1 %></th>
      <td><%= user.email %></td>
      <td><%= user.message %></td>
    </tr>

    <% }) %>
  </tbody>
</table>

<%- include("partials/footer.ejs") %>
